package com.team6.ecommercebackend.entities;

import java.util.List;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.OneToMany;
import javax.persistence.Table;

@Entity
@Table(name="orders")
public class Orders {
	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	@Column(name = "orders_id")
	private Long id;
	
	@Column(name = "order_details_id", nullable=false)
	private Long orderDetailsId;
	
	@Column(name = "product_id", nullable=false)
	private Long productId;
	
	@Column(name = "quantity", nullable=false)
	private Integer quantity;

	public Orders() {
		super();
	}

	public Orders(List<Long> orderDetailsId, List<Long> productId, Integer quantity) {
		super();
		this.orderDetailsId = orderDetailsId;
		this.productId = productId;
		this.quantity = quantity;
	}

	public Orders(Long id, List<Long> orderDetailsId, List<Long> productId, Integer quantity) {
		super();
		this.id = id;
		this.orderDetailsId = orderDetailsId;
		this.productId = productId;
		this.quantity = quantity;
	}

	

	
	
}
